<mvc:View controllerName="sydenoa.sydenoa.controller.LoginView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m" 
    xmlns:core="sap.ui.core">
    <Page id="page2" title="{i18n>title}">
    <content>
    <Label id="lblusuaurio" labelFor="txtusuario" text="Ingresar nombre usuario" />
